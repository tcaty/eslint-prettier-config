{
  "name": "@tcaty/eslint-prettier-config",
  "version": "1.0.0",
  "description": "Hi! This is my eslint prettier config :)",
  "main": "index.js",
  "repository": "git@github.com:tcaty/eslint-prettier-config.git",
  "author": "tcaty",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "publish-eslint-config": "yarn workspace @tcaty/prettier-config publish",
    "publish-prettier-config": "yarn workspace @tcaty/eslint-config publish",
    "publish-all": "concurrently \"yarn workspace @tcaty/eslint-config publish\" \"yarn workspace @tcaty/prettier-config publish\""
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
